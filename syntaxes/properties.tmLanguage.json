{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Java Properties",
	"patterns": [
		{
			"include": "#property-key"
		}
	],
	"repository": {
		"property-key": {
			"name": "keyword.other.properties",
			"match": "^\\w[\\w.-]*"
		}
	},
	"scopeName": "source.properties"
}
